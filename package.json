{
  "name": "basic-mmo-phaser",
  "version": "1.0.0",
  "description": " Very basic multiplayer online game example made with Phaser, Node.js and Socket.io ",
  "main": "server.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=development nodemon index.js --exec babel-node",
    "build": "npm run webpack && npm run webpack_server",
    "bs": "npm run build && npm run serve",
    "webpack_server": "cross-env NODE_ENV=production webpack --config webpack.config.server.js",
    "webpack": "cross-env NODE_ENV=production webpack --config webpack.config.prod.js",
    "serve": "cross-env NODE_ENV=production node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jerenaux/basic-mmo-phaser.git"
  },
  "keywords": [
    "Phaser",
    "Multiplayer",
    "game",
    "Node.js",
    "socket.io"
  ],
  "author": "Jerome Renaux",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jerenaux/basic-mmo-phaser/issues"
  },
  "homepage": "https://github.com/Jerenaux/basic-mmo-phaser#readme",
  "dependencies": {
    "express": "^4.15.2",
    "html-webpack-plugin": "^2.30.1",
    "p2": "^0.7.1",
    "socket.io": "^1.7.3",
    "socket.io-client": "^2.0.3",
    "webpack-externals-plugin": "^1.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.1.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "cross-env": "^5.0.5",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-import": "^2.7.0",
    "expose-loader": "^0.7.3",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "nodemon": "^1.11.0",
    "webpack": "^3.5.4",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.7.1",
    "webpack-node-externals": "^1.6.0"
  }
}
